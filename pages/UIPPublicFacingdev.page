<apex:page controller="UIPPublicFacingController" showHeader="false" applyBodyTag="false" applyHtmlTag="false" standardStylesheets="false" docType="HTML-5.0">
    <html lang="en">
        <head>
            <meta name="viewport" content="width=device-width, initial-scale=1" standardstylesheets="false"/>
            <meta charset="utf-8"/>
            <title>CO UIP</title>
            <apex:stylesheet value="{!URLFOR($Resource.UIP_assets, '/css/bootstrap.min.css')}"/>
            <apex:stylesheet value="{!URLFOR($Resource.UIP_assets, '/css/custom.css')}"/>
            <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"/>
            <apex:includeScript value="{!URLFOR($Resource.UIP_assets, '/js/bootstrap.min.js')}"/>
            <script>
            document.getElementById('{!$Component.PiInput}').disabled = true;
            </script>
            <style>
        body {
            word-wrap:initial;
        }
        @media (min-width: 768px) {
        .row.equal {
        display: flex;
        flex-wrap: wrap;
        }
        }
        .table.custom tr td {
            border:0;
        }
        .blue {color:#498bc9;}
        .light-blue {color:#6ec4e7;}
        .orange {color:#ee7622;}
        .purple {color:#6d3b5d;}
        .green {color:#467a7c;}
        @media (max-width: 767px) {
            .affix {top:0;position:relative;}
        .tab-content {overflow:hidden;}
        }
        .custom-media-left {
            vertical-align: bottom;
        }
        .icons-list {
            border: 1px solid #e7e8ea;
            padding: 15px;
            border-radius:3px;
        }
        .icons-list li {
            margin-bottom:25px;
        }
        .icons-list li img {
            margin:5px auto;
        }
        
        .icons-list li h4 {
            border-bottom: 1px solid #e7e8ea;
            padding-bottom: 10px;
        }
        .unbold {
            font-weight: normal;
         }
                .grey-box {
                text-align:center;
                position: relative;
                background: #e7e8ea;
            border-radius:5px; 
            wodth:100px;
            }
            .grey-box:after {
                left: 100%;
                top: 50%;
                border: solid transparent;
                content: " ";
                height: 0;
                width: 0;
                position: absolute;
                pointer-events: none;
                border-color: rgba(231, 232, 234, 0);
                border-left-color: #e7e8ea;
                border-width: 15px;
                margin-top: -15px;
            }
    </style> 
        </head>        
        <body>
            <apex:form >
                <apex:outputpanel rendered="{!IF(currentId == null ,FALSE, TRUE)}">
                    <apex:pageBlock id="fullPage">
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-6"><apex:image url="{!URLFOR($Resource.UIP_assets, '/img/CDELogo.png')}" styleClass="brand-img"/></div>
                                <div class="col-xs-6 text-right">
                                    <apex:outputPanel rendered="{!isCommunityUser}" styleClass="pull-right">
                                        <apex:outputLink value="{!$Site.Prefix}/secur/logout.jsp" onclick="if(!goNext()) {return false};">Log Out</apex:outputLink>
                                    </apex:outputPanel>
                                </div>
                            </div>
                        </div>
                        <div class="title">
                            <div class="title-border"></div>
                            <div class="container">
                                <h1>Colorado's Unified Improvement Plan for {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "Districts", "Schools")} </h1>
                            </div>
                        </div>
                        <div class="subtitle">
                            <div class="container">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-9 col-lg-10">
                                        <!-- <apex:outputPanel rendered="{!NOT(isCommunityUser)}">
                                        <h3>{!uip.Name}</h3>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isCommunityUser}">
                                        <h3>{!uip.Name}</h3>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        </apex:outputPanel>-->
                                        <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017'}">
                                            <h3>District:</h3> <span>{!uip.DISTRICT_NAME__r.Name}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <h3>Org ID:</h3> <span>{!uip.DIST_NUMBER__c}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <h3>Framework:</h3> <span>{!uip.A_TTL_RATING_OFFICIAL__c}</span>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017' || uip.RecordType.Name == 'School UIP 2017'}">
                                            <h3>District:</h3> <span>{!uip.DISTRICT_NAME__r.Name}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <h3>Org ID:</h3> <span>{!uip.DIST_NUMBER__c}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <h3>School ID:</h3> <span>{!uip.SCHOOL_NUMBER__c}</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                            <h3>Framework:</h3> <span>{!uip.A_TTL_RATING_OFFICIAL__c}</span>
                                        </apex:outputPanel>
                                    </div>
                                    <div class="col-=xs-12 col-sm-3 col-lg-2 text-right">
                                        <apex:commandlink action="{!downloadPDF}" target="_blank">
                                            <span class="glyphicon glyphicon glyphicon-download"></span>&nbsp;Download PDF
                                        </apex:commandlink> 
                                    </div>
                                </div>
                            </div>
                        </div>                        
                        <div class="container">
                            <apex:actionFunction name="rerenderMainSection" action="{!populatePPCTable}" status="loadingStatus" />
                            <div class="row">                                
                                <!-- Sidebar Nav -->
                                <div class="col-xs-12 col-sm-4 col-lg-3">
                                    <div class="fixed-nav">
                                        <h3>Content</h3>
                                        <ul class="list-unstyled nav sidebar-nav" role="tablist">
                                            <li role="presentation" class="active"><a href="#summary" aria-controls="summary" role="tab" data-toggle="tab">Executive Summary</a></li>
                                            <li role="presentation"><a href="#improvement" aria-controls="improvement" role="tab" data-toggle="tab">Improvement Plan Information</a></li>
                                            <li role="presentation"><a href="#narrative" aria-controls="narrative" role="tab" data-toggle="tab">Narrative on Data Analysis and Root Cause Identification</a></li>
                                            <li role="presentation"><a href="#action" aria-controls="action" role="tab" data-toggle="tab">Action Plans</a></li>
                                            <li role="presentation"><a href="#addenda" aria-controls="addenda" role="tab" data-toggle="tab">Addenda</a></li>
                                            <li role="presentation"><a href="#attachments" aria-controls="attachments" role="tab" data-toggle="tab">Attachments</a></li>
                                        </ul>                                        
                                        <br/>                                        
                                        <ul class="list-unstyled text-center icons-list">
                                            <li><h4 class="uppercase">Document icons and definitions</h4></li>
                                            <li class="divider"></li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/PriorityPerformanceChallenge.png" class="media-object sm"/>
                                                Priority Performance Challenges
                                            </li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/RootCause.png" class="media-object sm"/>
                                                Root Cause
                                            </li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/MajorImprovementStrategy.png" class="media-object sm"/>
                                                Major Improvement Strategies
                                            </li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/ActionStep.png" class="media-object sm"/>
                                                Action Steps
                                            </li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/ProgressMonitoring.png" class="media-object sm"/>
                                                Progress Monitoring
                                            </li>
                                            <li>
                                                <img src="/resource/1503663840000/UIP_assets/img/TrendDirection.png" class="media-object sm"/>
                                                Trend Direction
                                            </li>
                                        </ul>
                                    </div>
                                </div>                                
                                <!-- Tabs Content -->
                                <div class="col-xs-12 col-sm-8 col-lg-9 tab-content">                                    
                                    <!-- EXECUTIVE SUMMARY -->
                                    <div role="tabpanel" class="tab-pane active" id="summary">
                                        <h1 class="blue">Executive Summary</h1><br/>
                                        <apex:outputPanel layout="none" rendered="{!misWrapperList.size>0}">
                                            <section>
                                                <h2 class="blue">If we...</h2>                                                
                                                <div class="row equal">
                                                    <apex:repeat value="{!misWrapperList}" var="misWrapper">
                                                        <div class="col-xs-12 col-md-6">
                                                            <div style="background-color: #ebf4f4;padding: 10px;margin:10px 0;">
                                                                <h4 class="uppercase"><apex:outputText value="{!misWrapper.mis.Name}" /></h4>
                                                                <h4>{!IF(misWrapper.mis.Description__c != '' && misWrapper.mis.Description__c != null, "Description:", "")}</h4>
                                                                <div class="media">
                                                                    <div class="media-body">
                                                                        <apex:outputText value="{!misWrapper.mis.Description__c}" />
                                                                    </div>
                                                                    <div class="media-left custom-media-left">
                                                                        <a href="#"><img src="/resource/1503663840000/UIP_assets/img/MajorImprovementStrategy.png" class="media-object sm"/>
                                                                        </a>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </apex:repeat>
                                                </div>
                                            </section>
                                        </apex:outputPanel>
                                        <apex:outputPanel layout="none" rendered="{!rootcauseList.size>0}">
                                            <section>
                                                <h2 class="blue">Then we will address...</h2>                                                
                                                <div class="row equal">
                                                    <apex:repeat value="{!rootcauseList}" var="rc">
                                                        <div class="col-xs-12 col-md-6">
                                                            <div style="background-color: #ebf4f4; padding:10px;margin:10px 0;">
                                                                <h4 class="uppercase"><apex:outputtext value="{!rc.Name}"/></h4>
                                                                <h4>{!IF(rc.Description__c != '' && rc.Description__c != null, "Description:", "")}</h4>
                                                                <div class="media">
                                                                    <div class="media-body">
                                                                        <apex:outputtext value="{!rc.Description__c}"/>
                                                                    </div>
                                                                    <div class="media-left custom-media-left">
                                                                        <a href="#"><img src="/resource/1503663840000/UIP_assets/img/RootCause.png" class="media-object sm"/>
                                                                        </a>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <br/>
                                                    </apex:repeat> 
                                                </div>
                                            </section>
                                        </apex:outputPanel>
                                        <apex:outputPanel layout="none" rendered="{!challengeMapsize>0}"> 
                                            <section>
                                                <h2 class="blue">Then we will change current trends for students.</h2>                                                
                                                <div class="row equal">
                                                    <apex:repeat value="{!challengeMap}" var="challenge">
                                                        <div class="col-xs-12 col-md-6">
                                                            <div style="background-color: #ebf4f4;padding:10px;margin:10px 0;">
                                                                <h4 class="uppercase"><apex:outputText value="{!challengeMap[challenge].Name}" /></h4>
                                                                <h4>Description:</h4>
                                                                <div class="media">
                                                                    <div class="media-body">
                                                                        <apex:outputText value="{!challengeMap[challenge].Description__c}" />
                                                                    </div>
                                                                    <div class="media-left custom-media-left">
                                                                        <a href="#"><img src="/resource/1503663840000/UIP_assets/img/PriorityPerformanceChallenge.png" class="media-object sm"/>
                                                                        </a>
                                                                    </div>
                                                                </div>
                                                                
                                                            </div>
                                                        </div>
                                                    </apex:repeat>
                                                </div>
                                            </section>
                                        </apex:outputPanel>
                                        <p>
                                            Access the {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "District", "School")} Performance Framework here:
                                            <a href="http://www.cde.state.co.us/schoolview/performance">http://www.cde.state.co.us/schoolview/performance</a>
                                        </p>                                        
                                    </div>                                    
                                    <!-- IMPROVEMENT PLAN -->
                                    <div role="tabpanel" class="tab-pane" id="improvement">
                                        <h1 class="orange">Improvement Plan Information</h1><br/>                                        
                                        <section>
                                            <h2 class="orange">Additional Information about the {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district", "school")} </h2>
                                            <br/>   <!-- Vertiba - AK added 1/8/18 per SR 7278-->
                                            <apex:outputfield value="{!uip.Context__c}" ></apex:outputfield>
                                            <br/>
                                            <div class="row">
                                                <apex:outputPanel layout="none" rendered="{!uip.Related_Grant_Awards__c != null}">
                                                    <div class="col-xs-12 col-sm-6">
                                                        <div class="panel panel-default">
                                                            <div class="panel-body">
                                                                <h4 class="uppercase">Related Grant Awards</h4>
                                                                <p>Has the {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district", "school")} received a grant that supports {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district", "school")} improvement efforts? When was the grant awarded?</p>
                                                                <span><i><apex:outputField value="{!uip.Related_Grant_Awards__c}" /></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel layout="none" rendered="{!uip.SST_Expedited_Review__c != null}">
                                                    <div class="col-xs-12 col-sm-6">
                                                        <div class="panel panel-default">
                                                            <div class="panel-body">
                                                                <h4 class="uppercase">{!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', 'CADI' , 'School Support Team or Expedited Review')}</h4>
                                                                <p>{!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', 'Has (or will) the district participated in a CADI review? If so, when?' , 'Has (or will) the school participated in an SST or Expedited Review? If so, when?')}</p>
                                                                <span><i><apex:outputField value="{!uip.SST_Expedited_Review__c}" /></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel layout="none" rendered="{!uip.External_Evaluator__c != null}">
                                                    <div class="col-xs-12 col-sm-6">
                                                        <div class="panel panel-default">
                                                            <div class="panel-body">
                                                                <h4 class="uppercase">External Evaluator</h4>
                                                                <p>Has the {!IF(uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district", "school")} partnered with an external evaluator to provide comprehensive evaluation? Indicate the year and the name of the provider/tool used.</p> 
                                                                <span><i><apex:outputField value="{!uip.External_Evaluator__c}" /></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                            </div>
                                        </section>                                        
                                        <section>
                                            <h2 class="orange">Improvement Plan Information</h2>                                            
                                            <p>The school/district is submitting this improvement plan to satisfy requirements for (check all that apply):</p> 
                                            <br/>                 
                                            <apex:outputPanel layout="none" rendered="{!uip.State_Accreditation__c == true}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.State_Accreditation__c}" ></apex:outputfield>
                                                    <label>State Accreditation</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.Title_I_Focus_School__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Title_I_Focus_School__c}" >
                                                    </apex:outputfield>
                                                    <label>Title I Focus School</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Title_I_Focus_School__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Title_I_Focus_School__c}" >
                                                    </apex:outputfield>
                                                    <label>Title I Focus School</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.Biennial_Plan_Eligible__c == true}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Biennial_Plan_Eligible__c}" >
                                                    </apex:outputfield>
                                                    <label>Biennial Plan Eligible</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.Combined_Plan__c == true}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Combined_Plan__c}" >
                                                    </apex:outputfield>
                                                    <label>Combined Plan</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Student_Graduation_and_Completion_Plan__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Student_Graduation_and_Completion_Plan__c}">
                                                    </apex:outputfield>
                                                    <label>Student Graduation and Completion Plan (Designated Graduation District)</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.Tiered_Intervention_Grant_TIG__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Tiered_Intervention_Grant_TIG__c}" >
                                                    </apex:outputfield>
                                                    <label>Tiered Intervention Grant (TIG)</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Tiered_Intervention_Grant_TIG__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Tiered_Intervention_Grant_TIG__c}" >
                                                    </apex:outputfield>
                                                    <label>Tiered Intervention Grant (TIG)</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Title_IA__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Title_IA__c}" >
                                                    </apex:outputfield>
                                                    <label>Title IA</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.Colorado_Graduation_Pathways_Program__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Colorado_Graduation_Pathways_Program__c}" >
                                                    </apex:outputfield>
                                                    <label>Colorado Graduation Pathways Program (CGP)</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Colorado_Graduation_Pathways_Program__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Colorado_Graduation_Pathways_Program__c}" >
                                                    </apex:outputfield>
                                                    <label>Colorado Graduation Pathways Program (CGP)</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Title_IIA__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Title_IIA__c}" >
                                                    </apex:outputfield>
                                                    <label>Title IIA</label>
                                                </div>
                                            </apex:outputPanel>                                            
                                            <apex:outputPanel rendered="{!uip.School_Improvement_Support_Grant__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.School_Improvement_Support_Grant__c}" >
                                                    </apex:outputfield>
                                                    <label>School Improvement Support Grant</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.School_Improvement_Support_Grant__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.School_Improvement_Support_Grant__c}" >
                                                    </apex:outputfield>
                                                    <label>School Improvement Support Grant</label>
                                                </div>
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!uip.Title_III_Checkbox__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                                                <div class="form-group">
                                                    <apex:outputfield value="{!uip.Title_III_Checkbox__c}" >
                                                    </apex:outputfield>
                                                    <label>Title III</label>
                                                </div>
                                            </apex:outputPanel>                                            
