<apex:page controller="UIPReviewController" showHeader="false" applyBodyTag="false" applyHtmlTag="false" renderAs="pdf">
    <head>  
        <style>
            @page { size: A4; margin: 8mm; }
            .PDFpagebreak {page-break-before: always;}
            body {
            font-family: Helvetica, sans-serif;
            margin:0;
            padding:0;
            font-size:14px;
            color:#5c666f;
            }
            .spacing {
            height:30px;
            }
            .spacing-half {
            height:15px;
            }
            .section {
            padding:0 10px;
            }
            .title {
            width:100%;
            background-color:#488bc9;
            color:#fff;
            padding:1.5mm;
            }
            h1 {font-size:26px; color:#32383e;}
            h2 {font-size:20px;}
            h3 {font-size:16px; color:#488bc9;margin-top:20px;}
            h4 {font-size:13px; text-transform:uppercase;color:#32383e;}
            h5 {font-size:14px; font-weight:bold;margin-bottom:1mm;}
            .list-unstyled {
            margin-left:0;
            list-style-type:none;
            }
            .list-unstyled li {
            margin:1mm 0;
            }
        </style>
    </head>
    
    <body>
        <apex:pageBlock id="fullPage">
            
            <!-- Header -->
            <table width="100%" >
                <tr>
                    <td>
                        <img src="{!$Resource.CDELogo}" width="250"/>
                    </td>
                    <td style="text-align:right;">
                        <img height="100%" src="{!$Resource.OFP_UIP}" width="120"/>
                    </td>
                </tr>
                <tr><td colspan="2" class="spacing-half"></td></tr>
                <tr>
                    <td colspan="2" style="text-align:center;">
                        <h1 style="margin-bottom:0;">Colorado's Unified Improvement Plan
                            for {!IF(uip.RecordType.Name == 'District UIP' ||
                            uip.RecordType.Name == 'District UIP 2015' ||
                            uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "District",
                            "School")} </h1><br/>
                        
                        <b>{!uip.Name}</b> &nbsp; | &nbsp;
                        <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017'}">
                            <b>District:</b> {!uip.DISTRICT_NAME__r.Name} &nbsp; | &nbsp;
                            <b>Org ID:</b> {!uip.DIST_NUMBER__c} &nbsp; | &nbsp;
                            <b>Framework:</b> {!uip.A_TTL_RATING_OFFICIAL__c} &nbsp; | &nbsp;
                            <b>Draft UIP</b>
                        </apex:outputPanel>
                        
                        <apex:outputPanel rendered="{!uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'School UIP 2016'|| uip.RecordType.Name == 'AEC UIP 2017' || uip.RecordType.Name == 'School UIP 2017'}">
                            <b>District:</b> {!uip.DISTRICT_NAME__r.Name} &nbsp; | &nbsp;
                            <b>Org ID:</b> {!uip.DIST_NUMBER__c} &nbsp; | &nbsp;
                            <b>School ID:</b> {!uip.SCHOOL_NUMBER__c} &nbsp; | &nbsp;
                            <b>Framework:</b> {!uip.A_TTL_RATING_OFFICIAL__c} &nbsp; | &nbsp;
                            <b>Draft UIP</b>
                        </apex:outputPanel>  
                    </td>
                </tr>
                <tr><td colspan="2" class="spacing"></td></tr>
                <tr>
                    <td colspan="2">
                        <h3>Table of Contents</h3>
                        <ul class="list-unstyled">
                            <li>Executive Summary</li>
                            <li>Improvement Plan Information</li>
                            <li>Narrative on Data Analysis and Root Cause Identification</li>
                            <li>Action Plans</li>
                            <li>Addenda</li>
                        </ul>
                    </td>	
                </tr>
                <tr><td colspan="2" class="spacing-half"></td></tr>
            </table>
            
            <!-- Page Content -->
            
            <!-- Executive Summary -->
            <h2 class="title">Executive Summary </h2>
            <div class="section">
                <apex:outputPanel layout="none" rendered="{!challengeMapsize>0}">
                    <h4>
                        How are students performing? Where will the
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} focus attention?
                    </h4>
                    
                    <p>
                        Priority Performance Challenges: Specific
                        statements about the {!IF(uip.RecordType.Name == 'District
                        UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017',
                        "district's", "school's")} performance challenges (not
                        budgeting, staffing curriculum, instruction, etc.), with at
                        least one priority identified for each performance indicator
                        (Achievement, Growth, PWR), where the
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "District",
                        "School")} did not meet federal, state and/or local
                        expectations.
                    </p> 
                    <apex:repeat value="{!challengeMap}" var="challenge">
                        <apex:outputPanel >
                            <b>Name:</b>
                            <apex:outputText value="{!challengeMap[challenge].Name}" /><br /> 
                            <b>Description:</b> 
                            <apex:outputText value="{!challengeMap[challenge].Description__c}" />
                        </apex:outputPanel>
                    </apex:repeat>
                </apex:outputPanel>
                
                <apex:outputPanel layout="none" rendered="{!rootcauseList.size>0}">
                    <h4>
                        Why is the education system continuing to have these challenges?
                    </h4>
                    <p>
                        Root Causes: Statements describing the deepest underlying cause, or
                        causes, or performance challenges, that, if dissolved, would result in
                        elimination, or substantial reduction of the performance challenge(s).
                    </p>
                    <apex:repeat value="{!rootcauseList}" var="rc">
                        <apex:outputpanel >
                            <b>Name:</b>&nbsp;
                            <apex:outputtext value="{!rc.Name}"/><br/>
                            <b>Description:</b> &nbsp;
                            <apex:outputtext value="{!rc.Description__c}"/>
                        </apex:outputpanel>
                    </apex:repeat>
                </apex:outputPanel>
                
                <apex:outputPanel layout="none" rendered="{!misWrapperList.size>0}">
                    <h4>
                        Major Improvement Strategies
                    </h4>
                    <p>
                        Major Improvement Strategies: Identify the
                        major improvement strategy(s) 
                        that will address the root causes determined in the data
                        narrative.
                    </p>
                    <apex:repeat value="{!misWrapperList}" var="misWrapper">
                        <apex:outputPanel >
                            <b>Name:</b> &nbsp;
                            <apex:outputText value="{!misWrapper.mis.Name}" /><br />
                            <b>Description:</b> &nbsp;
                            <apex:outputText value="{!misWrapper.mis.Description__c}" />
                        </apex:outputPanel>
                    </apex:repeat>
                </apex:outputPanel>
                
                <p>
                    Access the {!IF(uip.RecordType.Name == 'District UIP' ||
                    uip.RecordType.Name == 'District UIP 2015' ||
                    uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "District",
                    "School")} Performance Framework here:<a href="http://www.cde.state.co.us/schoolview/performance">http://www.cde.state.co.us/schoolview/performance</a>
                </p>
                
            </div>
            <br/><br/>
            
            <!-- Improvement Plan Information -->
            <h2 class="title">Improvement Plan Information</h2>
            <div class="section">
                
                <h3>Additional
                    Information about the {!IF(uip.RecordType.Name == 'District
                    UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017',
                    "district", "school")}
                </h3>
                
                <h5>Comprehensive Review and Selected Grant History</h5>
                
                <apex:outputPanel layout="none" rendered="{!uip.Related_Grant_Awards__c != null}">
                    <h5>
                        Related Grant Awards
                    </h5>
                    <p>Has the {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} received a grant that supports
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} improvement efforts? When was the grant awarded?</p> <span><i><apex:outputField value="{!uip.Related_Grant_Awards__c}" /></i></span>
                </apex:outputPanel>
                <apex:outputPanel layout="none" rendered="{!uip.SST_Expedited_Review__c != null}">
                    <h5>
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016'  || uip.RecordType.Name == 'District UIP 2017', 'CADI' ,
                        'School Support Team or Expedited Review')}
                    </h5>
                    <p>{!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016'  || uip.RecordType.Name == 'District UIP 2017', 'Has (or will)
                        the district participated in a CADI review? If so, when?' ,
                        'Has (or will) the school participated in an SST or Expedited
                        Review? If so, when?')}</p>
                    <apex:outputField value="{!uip.SST_Expedited_Review__c}" />
                </apex:outputPanel>
                <apex:outputPanel layout="none" rendered="{!uip.External_Evaluator__c != null}">
                    <h5>
                        External Evaluator
                    </h5>
                    <p>Has the {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} partnered with an external evaluator to provide
                        comprehensive evaluation? Indicate the year and the name of
                        the provider/tool used.</p>
                    <apex:outputField value="{!uip.External_Evaluator__c}" />
                </apex:outputPanel>
                
                <p>The school/district is submitting this improvement plan to satisfy requirements for (check all that apply):</p>
                
                <ul class="list-unstyled">
                    <apex:outputPanel layout="none" rendered="{!uip.State_Accreditation__c == true}">
                        <li><apex:outputField value="{!uip.State_Accreditation__c}" /> State Accreditation</li>
                    </apex:outputPanel> 
                    <apex:outputPanel rendered="{!uip.Title_I_Focus_School__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                        <li><apex:outputField value="{!uip.Title_I_Focus_School__c}" /> Title I Focus School</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Title_I_Focus_School__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016'  || uip.RecordType.Name == 'AEC UIP 2017')}">
                        <li><apex:outputField value="{!uip.Title_I_Focus_School__c}" /> Title I Focus School</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Student_Graduation_and_Completion_Plan__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                        <li><apex:outputField value="{!uip.Student_Graduation_and_Completion_Plan__c}" /> Student Graduation and Completion Plan (Designated Graduation District) </li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Tiered_Intervention_Grant_TIG__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                        <li><apex:outputField value="{!uip.Tiered_Intervention_Grant_TIG__c}" /> Tiered Intervention Grant (TIG)</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Tiered_Intervention_Grant_TIG__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                        <li><apex:outputField value="{!uip.Tiered_Intervention_Grant_TIG__c}" /> Tiered Intervention Grant (TIG)</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Title_IA__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                        <li><apex:outputField value="{!uip.Title_IA__c}" /> Title IA</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Colorado_Graduation_Pathways_Program__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016' || uip.RecordType.Name == 'School UIP 2017')}">
                        <li><apex:outputField value="{!uip.Colorado_Graduation_Pathways_Program__c}" /> Colorado Graduation Pathways Program (CGP)</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Colorado_Graduation_Pathways_Program__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                        <li><apex:outputField value="{!uip.Colorado_Graduation_Pathways_Program__c}" /> Colorado Graduation Pathways Program (CGP)</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Title_IIA__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                        <li><apex:outputField value="{!uip.Title_IIA__c}" /> Title IIA</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.School_Improvement_Support_Grant__c == true && (uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016'  || uip.RecordType.Name == 'School UIP 2017')}">
                        <li><apex:outputField value="{!uip.School_Improvement_Support_Grant__c}" /> School Improvement Support Grant</li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.School_Improvement_Support_Grant__c == true && (uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016' || uip.RecordType.Name == 'AEC UIP 2017')}">
                        <li><apex:outputField value="{!uip.School_Improvement_Support_Grant__c}" /> School Improvement Support Grant </li>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Title_III_Checkbox__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                        <li><apex:outputField value="{!uip.Title_III_Checkbox__c}" /> Title III</li> 
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!uip.Gifted_Education_Checkbox__c == true && (uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017')}">
                        <li><apex:outputField value="{!uip.Gifted_Education_Checkbox__c}" /> Gifted Education</li>
                    </apex:outputPanel>
                    <apex:outputPanel layout="none" rendered="{!uip.Other__c == true}">
                        <li><apex:outputField value="{!uip.Other__c}"></apex:outputField> Other</li>
                    </apex:outputPanel>
                    <apex:outputpanel layout="none" rendered="{!uip.Other_Description__c != null}">
                        <li><apex:outputField value="{!uip.Other_Description__c}" /></li>
                    </apex:outputpanel>
                </ul>
                
                <apex:outputPanel layout="none" rendered="{!uipContactsList.size>0}">
                    <h3>
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016', "District",
                        "School")} Contact Information
                    </h3>
                    
                    <apex:repeat value="{!uipContactsList}" var="item">
                        <table>
                            <tr>
                                <td>
                                    Name:<br/>
                                    {!item.First_Name__c} &nbsp;{!item.Last_Name__c}<br />
                                </td>
                                <td>
                                    Title:<br/>
                                    {!item.Title__c}<apex:outputPanel rendered="{!item.Title__c != null}"></apex:outputPanel>
                                </td>
                                <td>
                                    Mailing Street:<br/>
                                    {!item.Mailing_Street__c}<apex:outputPanel rendered="{!item.Mailing_Street__c != null}"> </apex:outputPanel>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    Mailing City / State/ Zip Code:<br/>
                                    {!item.Mailing_City__c} {!item.Mailing_State_Province__c} {!item.Mailing_Zip_Postal_Code__c}
                                    <apex:outputPanel rendered="{!item.Mailing_City__c != null || item.Mailing_State_Province__c != null || item.Mailing_Zip_Postal_Code__c != null}"></apex:outputPanel>
                                </td>
                                <td>
                                    Phone:<br/>
                                    <apex:outputPanel rendered="{!item.Phone__c != null}"> </apex:outputPanel>
                                </td>
                                <td>
                                    Email:<br/>
                                    {!item.Email__c}<apex:outputPanel rendered="{!item.Email__c != null}"></apex:outputPanel>
                                </td>
                            </tr>
                        </table>
                    </apex:repeat>
                </apex:outputPanel>
            </div>
            <br/><br/>
            
            <!-- Narrative on Data Analysis and Root Cause Identification -->
            <h2 class="title">Narrative on Data Analysis and Root Cause Identification</h2>
            <div class="section">
                <apex:outputPanel layout="none" rendered="{!uip.TextAreaRich_Test__c != null}">
                    <h3>
                        Description of {!IF(uip.RecordType.Name == 'District
                        UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017',
                        "district", "school")} Setting and Process for Data Analysis
                    </h3>
                    
                    <p>
                        Provide a brief description
                        of the {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} to set the context for readers. Include the general
                        process for developing the UIP and participants (e.g.,
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "DAC", "SAC")}
                        involvement). The description may include demographics and
                        local context, such as location, performance status, notable
                        recent events or changes, stakeholders involved in writing the
                        UIP, and an overview of the general process.
                    </p>
                    
                    <apex:outputField value="{!uip.TextAreaRich_Test__c}" />
                </apex:outputPanel>
                <br/>
                <apex:outputPanel layout="none" rendered="{!pitTargetsMapsize>0}">
                    <h3>Prior Year Targets</h3>
                    <p>
                        Consider
                        the previous year's progress toward the
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} targets. Identify the overall magnitude of the
                        {!IF(uip.RecordType.Name == 'District UIP' ||
                        uip.RecordType.Name == 'District UIP 2015' ||
                        uip.RecordType.Name == 'District UIP 2016' || uip.RecordType.Name == 'District UIP 2017', "district",
                        "school")} performance challenges.
                    </p>
                    
                    <apex:repeat value="{!pitTargetsMap}" var="target">
                        <div style="background-color:#f0f9fd;padding:10px;padding-top:5px;padding-bottom:15px;">
                            <apex:variable value="{!pitTargetsMap[target]}"  var="pitTargetsMapList" />
                            <apex:outputPanel layout="none" rendered="{!pitTargetsMapList.size>0}">
                                <h4>Performance Indicator:&nbsp;&nbsp;{!target}</h4>
                                <table>
                                    <apex:repeat value="{!pitTargetsMap[target]}" var="pit">
                                        <tr>
                                            <td>
                                                <b>Prior Year Target:</b>
                                            </td>
                                            <td>
                                                <apex:outputField value="{!pit.Last_Year_Target_1__c}" />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <b>Performance:</b>
                                            </td>
                                            <td>
                                                <apex:outputField value="{!pit.Performance_against_Target_1__c}" />
                                            </td>
                                        </tr>
                                    </apex:repeat>
                                </table>
                            </apex:outputPanel>
                            <apex:outputPanel layout="none" rendered="{!(target == 'Academic Achievement (Status)' && uip.Narrative_Academic_Achievement_Targets__c != null) ||
                                                                      (target == 'Academic Growth' && uip.Narrative_Academic_Growth_Targets__c != null) ||
                                                                      (target == 'Disaggregated Growth' && uip.Narrative_Academic_Growth_Gaps_Target__c != null) ||
                                                                      (target == 'Postsecondary & Workforce Readiness' && uip.Narrative_Postsecondary_Workforce_Target__c != null) ||
                                                                      (target == 'English Language Development and Attainment' && uip.Narrative_English_Lang_Dev_Attain_Target__c != null) ||
                                                                      (target == 'Student Engagement' && uip.Narrative_Student_Engagement_Target__c != null) ||
                                                                      (target == 'Disaggregated Achievement' && uip.Narrative_Disaggregated_Achievement__c != null) ||
                                                                      (target == 'Student Behavior' && uip.Narrative_Student_Behavior__c != null) ||
                                                                      (target == 'Other' && uip.Narrative_Other__c != null)}">
                                <table>
                                    <tr>
                                        <td>
                                            <b>{!target} Reflection</b> 
                                        </td>
                                        <td>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'Academic Achievement (Status)'}">
                                                <apex:outputField value="{!uip.Narrative_Academic_Achievement_Targets__c}" />
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'Academic Growth'}">
                                                <apex:outputField value="{!uip.Narrative_Academic_Growth_Targets__c}" />
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'Disaggregated Growth'}">
                                                <apex:outputField value="{!uip.Narrative_Academic_Growth_Gaps_Target__c}" />
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'Postsecondary & Workforce Readiness'}">
                                                <apex:outputField value="{!uip.Narrative_Postsecondary_Workforce_Target__c}" />
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'English Language Development and Attainment'}">
                                                <apex:outputField value="{!uip.Narrative_English_Lang_Dev_Attain_Target__c}" />
                                            </apex:outputPanel>
                                            <apex:outputPanel rendered="{!NOT(isEditRTF) && target == 'Student Engagement'}">
                                                <apex:outputField value="{!uip.Narrative_Student_Engagement_Target__c}" />
                                            </apex:outputPanel>
                                            <apex:outputpanel rendered="{!NOT(isEditRTF) && target == 'Disaggregated Achievement'}">
                                                <apex:outputfield value="{!uip.Narrative_Disaggregated_Achievement__c}"/>
