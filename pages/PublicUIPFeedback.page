                                <apex:outputPanel >
                                            {!item.First_Name__c} &nbsp;{!item.Last_Name__c}<br />
                                            {!item.Title__c}<apex:outputPanel rendered="{!item.Title__c != null}">
                                        <br />
                                    </apex:outputPanel>
                                            {!item.Mailing_Street__c}<apex:outputPanel rendered="{!item.Mailing_Street__c != null}">
                                        <br />
                                    </apex:outputPanel>
                                            {!item.Mailing_City__c} {!item.Mailing_State_Province__c} {!item.Mailing_Zip_Postal_Code__c}<apex:outputPanel rendered="{!item.Mailing_City__c != null || item.Mailing_State_Province__c != null || item.Mailing_Zip_Postal_Code__c != null}">
                                        <br />
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!item.Phone__c != null}">Phone:</apex:outputPanel> {!item.Phone__c}<apex:outputPanel rendered="{!item.Phone__c != null}">
