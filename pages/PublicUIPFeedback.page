                        </tr>
                        <tr>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Colorado_Graduation_Pathways_Program__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Colorado Graduation Pathways Program (CGP)</td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Colorado_Graduation_Pathways_Program__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Colorado Graduation Pathways Program (CGP)</td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Title_IIA__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Title IIA</td>
                            </apex:outputPanel>
                        </tr>
                        <tr>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.School_Improvement_Support_Grant__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>School Improvement Support Grant</td>
                            </apex:outputPanel>
                             <apex:outputPanel rendered="{!uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.School_Improvement_Support_Grant__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>School Improvement Support Grant</td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Title_III_Checkbox__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Title III</td>
                            </apex:outputPanel>
                        </tr>
                        <tr>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Gifted_Education_Checkbox__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Gifted Education</td>
                            </apex:outputPanel>
                        </tr>
                        <tr>
                            <td width="10%" class="section2-element-col">
                                <apex:outputfield value="{!uip.Other__c}">
                                    <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                </apex:outputfield>
                            </td>
                            <td>
                                <div style="display: inline-block; vertical-align: top; padding-top:5px;">Other &nbsp;&nbsp;</div><br/><br/>
                               <apex:outputfield value="{!uip.Other_Description__c}" id="otherTxt" style=" visibility:hidden; width:400px;"/>
                            </td> 
                            <script type="text/javascript">
                                window.onload=function(){
                                    ckb = document.getElementById("{!$Component.OtherCbx}").checked;
                                    txt = document.getElementById("{!$Component.otherTxt}");
                                    if (ckb) {
                                        txt.style.visibility = "Visible";
                                    }
                                };
                                
                                function showOther() {
                                    ckb = document.getElementById("{!$Component.OtherCbx}").checked;
                                    txt = document.getElementById("{!$Component.otherTxt}");
                                    if (ckb) {
                                        txt.style.visibility = "Visible";
                                    } else {
                                        txt.style.visibility = "Hidden";
                                    }
