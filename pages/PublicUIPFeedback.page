                        <tr>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'School UIP' || uip.RecordType.Name == 'School UIP 2015' || uip.RecordType.Name == 'School UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Tiered_Intervention_Grant_TIG__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Tiered Intervention Grant (TIG)</td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'AEC UIP' || uip.RecordType.Name == 'AEC UIP 2015' || uip.RecordType.Name == 'AEC UIP 2016'}">
                                <td width="10%" class="section2-element-col">
                                    <apex:outputfield value="{!uip.Tiered_Intervention_Grant_TIG__c}" >
                                        <apex:actionSupport event="onchange" action="{!setPageChanged}" rerender="script" />
                                    </apex:outputfield>
                                </td><td>Tiered Intervention Grant (TIG)</td>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!uip.RecordType.Name == 'District UIP' || uip.RecordType.Name == 'District UIP 2015' || uip.RecordType.Name == 'District UIP 2016'}">
                                <td width="10%" class="section2-element-col">
